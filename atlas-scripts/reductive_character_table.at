<simple_character_table.at
<character_table_tensor.at

set character_table(RootDatum rd)= CharacterTable:
   let factors=rd.derived.simple_factors
   then table =
      if =#factors then error("Cannot handle triaivla Weyl group") fi
      ; simple_character_table(factors[0].derived)
   in for factor in factors[1:]
      do table:=tensor(table,simple_character_table(factor.derived))
      od; table

set character_table(RootDatum rd,ratvec infinitesimal_character)=
    CharacterTable:
let rd_int=integrality_datum(rd,infinitesimal_character) in
prints("integrality datum: ", rd_int); character_table(rd_int)